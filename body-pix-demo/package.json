{
  "name": "tfjs-models",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow-models/body-pix": "^2.1.0",
    "@tensorflow-models/posenet": "^2.1.3",
    "@tensorflow/tfjs-backend-webgl": "^3.1.0",
    "@tensorflow/tfjs-converter": "^3.1.0",
    "@tensorflow/tfjs-core": "^3.1.0",
    "stats.js": "0.17.0"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development parcel src/index.html --no-hmr --open",
    "build": "yarn run clean && yarn run parcel:build && yarn run workbox:generate",
    "clean": "rimraf dist/",
    "parcel:build": "cross-env NODE_ENV=production parcel build src/index.html --no-source-maps --public-url ./",
    "workbox:generate": "workbox generateSW workbox-config.js",
    "lint": "eslint ."
  },
  "browser": {
    "crypto": false
  },
  "devDependencies": {
    "babel-core": "~6.26.3",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "cross-env": "^5.2.0",
    "dat.gui": "~0.7.2",
    "eslint": "~7.20.0",
    "eslint-config-google": "~0.14.0",
    "mkdirp": "^1.0.4",
    "parcel-bundler": "~1.12.3",
    "parcel-plugin-pwa-manifest": "^3.0.10",
    "parcel-plugin-structurize": "^2.3.2",
    "rimraf": "^3.0.2",
    "workbox-cli": "^6.1.1",
    "workbox-window": "^6.1.1",
    "yalc": "~1.0.0-pre.23"
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ],
  "pwaManifest": {
    "name": "Virtual Background",
    "shortName": "free background",
    "startURL": "./",
    "theme": "#80cee8",
    "generateIconOptions": {
      "baseIcon": "./virtual-background-icon.svg",
      "purpose": [
        "any"
      ],
      "sizes": [
        144,
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "parcel-plugin-structurize": {
    "rules": [
      {
        "match": "*.{png,webp,svg}",
        "folder": "assets"
      }
    ]
  }
}
